{
  "datefull": {
    "prefix": ["datefull"],
    "body": [
      "$CURRENT_DAY_NAME_SHORT, $CURRENT_MONTH_NAME_SHORT $CURRENT_DATE $CURRENT_YEAR at $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND"
    ],
    "description": "Tue, Sep 26 2023 at 13:43:09"
  },
  "dateYYYYMMDD": {
    "prefix": ["YYYYMMDD", "dateYYYYMMDD"],
    "body": [
      "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE"
    ],
    "description": "2023-07-15"
  },
  "date": {
    "prefix": ["date"],
    "body": [
      "$CURRENT_MONTH_NAME $CURRENT_DATE $CURRENT_YEAR"
    ],
    "description": "March 18 2023"
  },
  "filename" :{
    "prefix":["filename", "name", "fname"],
    "body":[
      "$TM_FILENAME"
    ],
    "description": "get curent filename"
  },
  "ANSI single line comment": {
    "prefix": ["/*"],
    "body": [
      "/* ${1} */"
    ],
    "description": "/* */ comment"
  },
  "ANSI multible lines comment": {
    "prefix": ["/**"],
    "body": [
      "/**",
      "* ${1}",
      "*/"
    ],
    "description": "/** */ multible lines comment"
  }
}
