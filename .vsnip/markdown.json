{
  "blog header": {
    "prefix": ["---", "title"],
    "body": [
      "---",
      "title: '$1'",
      "draft: ${2:true} # Set 'false' to publish",
      "tableOfContents: false # Enable/disable Table of Contents",
      "description: '$3'",
      "categories:",
      "    - $4",
      "tags:",
      "    - $5",
      "---",
    ]
  }
}
